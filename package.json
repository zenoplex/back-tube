{
  "name": "back-tube",
  "version": "0.0.1",
  "description": "Display YouTube video as background in any element.",
  "author": "zenoplex",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "scripts": {
    "clean": "rimraf lib dist",
    "browsersync": "browser-sync start --server --files 'dist/css/**/*.css, dist/**/*.html, dist/**/*.js'",
    "webpack:build": "webpack --config webpack.config.production.js",
    "webpack:watch": "webpack --hot --inline --watch",
    "watch": "npm run clean && parallelshell 'npm run webpack:watch' 'npm run browsersync'",
    "build:umd": "parallelshell 'npm run webpack:build'",
    "build:lib": "babel src/js -d lib",
    "build": "npm run build:lib && npm run build:umd",
    "prepublish": "npm run clean && npm run build",
    "gh-pages": "npm run build && mkdir ghpages && cp index.html ghpages && cp -R assets ghpages && cp -R dist ghpages && gh-pages -d ghpages && rimraf ghpages"
  },
  "keywords": [
    "YouTube",
    "babel",
    "webpack"
  ],
  "devDependencies": {
    "autoprefixer": "^6.1.1",
    "babel-cli": "^6.2.0",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "browser-sync": "^2.10.0",
    "gh-pages": "^0.5.0",
    "parallelshell": "^2.0.0",
    "path": "^0.12.7",
    "postcss-cli": "^2.3.2",
    "rimraf": "^2.4.4",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0",
    "yaml2json": "^1.0.2"
  },
  "dependencies": {
    "lodash.merge": "^3.3.2"
  },
  "engines": {
    "node": ">=4.2.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zenoplex/back-tube"
  }
}
